{
  "sourceMaps": true,
  "presets": [
    "@babel/typescript",
    [
      "@babel/preset-env",
      {
        "corejs": 3,
        "loose": true,
        "shippedProposals": true,
        "targets": {
          "browsers": "> 0.25%, not dead",
          "node": "9"
        },
        "useBuiltIns": "usage"
      },
      "minify"
    ]
  ],
  "plugins": ["@babel/plugin-transform-runtime"]
}
